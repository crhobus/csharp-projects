@model IEnumerable<SGAL.Dominio.Avaliacao>

@{
    ViewBag.Title = "Avaliações";
}

<h2>Avaliações</h2>

<p>
    @Html.ActionLink("Nova Avaliação", "Create", new { idMontagem = ViewBag.Montagem.MontagemID })
</p>
<table class="table">
    <tr>
        <th>
            Data
        </th>
        <th>
            Estrelas
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.DataComentario.ToString("dd/MM/yyyy")
        </td>
        <td>
            @for (int i = 0; i < item.QuantidadeEstrelas; i++)
            {
            <img src='@string.Format("data:image/png;base64,")' width="30" height="30" />
            }
            
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { idMontagem = ViewBag.Montagem.MontagemID, idAvaliacao = item.AvaliacaoID }) |
            @Html.ActionLink("Excluir", "Delete", new { idMontagem = ViewBag.Montagem.MontagemID, idAvaliacao = item.AvaliacaoID })
        </td>
    </tr>
}

</table>
